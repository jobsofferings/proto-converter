{
  "name": "proto-converter3",
  "version": "0.0.1",
  "description": "converts proto to GraphQL schema ",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "rm -rf lib && tsc -p ./tsconfig.build.json",
    "test-coverage": "jest --collectCoverage",
    "preversion": "npm run format && npm run lint && npm run test",
    "version": "npm run build",
    "postversion": "git push && git push --tags",
    "format": "prettier --write --parser typescript \"src/**/*.ts\"",
    "test": "jest",
    "lint": "eslint --ext .ts src"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint",
      "prettier --parser=typescript --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "bin": {
    "proto-converter": "./bin/proto-converter"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "protobuf",
    "protobufjs",
    "proto",
    "proto3",
    "graphql",
    "schema",
    "converter"
  ],
  "author": "jobsofferings",
  "license": "MIT",
  "homepage": "https://github.com/whale-labs/proto-converter/#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whale-labs/proto-converter.git"
  },
  "bugs": {
    "url": "https://github.com/whale-labs/proto-converter/issues"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "graphql": "15.2.0",
    "inquirer": "^7.3.3",
    "lodash": "^4.17.19",
    "protobufjs": "6.10.1",
    "shelljs": "^0.8.4",
    "chalk": "^4.1.0",
    "date-fns": "^2.14.0",
    "prettier": "^2.0.5",
    "@nestjs/cli": "^7.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/inquirer": "^7.3.0",
    "@types/jest": "^26.0.3",
    "@types/lodash": "^4.14.157",
    "@types/node": "^14.0.14",
    "@types/shelljs": "^0.8.8",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "husky": "^4.3.0",
    "jest": "^26.1.0",
    "lint-staged": "^10.5.1",
    "typescript": "^3.9.6"
  }
}
